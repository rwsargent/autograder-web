<html>
	<head>
		<script src="https://cdn.jsdelivr.net/npm/vue"></script>
		<script>
		var app = undefined;
			document.addEventListener("DOMContentLoaded", function() {
			app = new Vue({
				el : '#app',
				data : {
					message : 'fuck it' 
				}
			});
			});
		</script>
	</head>
	<body>
		<div id="app">
			{{ message }}
		</div>
	</body>
</html>